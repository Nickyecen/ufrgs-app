; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="ufrgs-app"
run/main_scene="res://scenes/login_page.tscn"
config/features=PackedStringArray("4.3", "C#", "Mobile")
run/low_processor_mode=true
boot_splash/bg_color=Color(0.901961, 0.235294, 0.168627, 1)
boot_splash/image="res://icon.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://icon.png"

[autoload]

HttpClientSingleton="*res://scripts/HttpClientSingleton.cs"

[display]

window/size/viewport_width=540
window/size/viewport_height=1080
window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/handheld/orientation=1

[dotnet]

project/assembly_name="ufrgs-app"

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
